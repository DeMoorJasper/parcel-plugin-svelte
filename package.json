{
  "name": "parcel-plugin-svelte",
  "version": "2.0.0",
  "description": "Parcel plugin to support svelte",
  "main": "./dist.js",
  "scripts": {
    "test": "yarn lint && NODE_ENV=test mocha --require babel-core/register --timeout 50000",
    "build": "babel src -d lib",
    "prepublish": "yarn build",
    "format": "prettier --write \"./{src,test,example}/**/*.{js,json,md}\""
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [
    "parcel",
    "svelte"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/DeMoorJasper/parcel-plugin-svelte.git"
  },
  "author": "Jasper De Moor",
  "license": "ISC",
  "peerDependencies": {
    "parcel-bundler": "^1.7.0",
    "svelte": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "mocha": "^5.1.1",
    "parcel-assert-bundle-tree": "^1.0.0",
    "parcel-bundler": "^1.7.1",
    "prettier": "^1.16.4",
    "svelte": "^2.4.2"
  },
  "dependencies": {
    "svelte-dev-helper": "^1.1.7"
  }
}
